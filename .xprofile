#!/bin/bash

# This file runs when you log in.

pulseaudio &
picom &                                     # for transparency
pgrep -x ibus-daemon || ibus-daemon -drx    # run ibus when startup
twmnd > /dev/null &                         # for notify program
#xset r rate 300 50 &	                    # Speed xrate up
while :; do                                 # change wallpaper every 10 minutes
    setbg ~/user/Pictures/Wallpapers
    sleep 10m
done &
